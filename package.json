{
  "name": "storefront",
  "title": "Storefront",
  "version": "2.9.0",
  "homepage": "http://woocommerce.com/storefront/",
  "repository": {
    "type": "git",
    "url": "https://github.com/woothemes/storefront.git"
  },
  "scripts": {
    "build": "grunt deploy --env=production",
    "build:dev": "grunt",
    "css": "grunt css",
    "start": "grunt watch",
    "wp-env": "wp-env",
    "e2e": "jest",
    "e2e:ci": "npm run wp-env start && npm run e2e"
  },
  "jest": {
    "preset": "jest-puppeteer",
    "setupFilesAfterEnv": [
      "expect-puppeteer"
    ],
    "globals": {
      "STORE_URL": "http://localhost:8802"
    }
  },
  "config": {
    "wp_org_slug": "storefront",
    "translate": true
  },
  "license": "GPL-2.0+",
  "main": "Gruntfile.js",
  "devDependencies": {
    "@lodder/grunt-postcss": "3.0.0",
    "@wordpress/env": "1.6.0",
    "autoprefixer": "10.0.2",
    "bourbon": "5.1.0",
    "cssnano": "4.1.10",
    "grunt": "1.3.0",
    "grunt-checktextdomain": "1.0.1",
    "grunt-contrib-compress": "1.6.0",
    "grunt-contrib-copy": "1.0.0",
    "grunt-contrib-cssmin": "3.0.0",
    "grunt-contrib-jshint": "3.0.0",
    "grunt-contrib-uglify": "4.0.1",
    "grunt-contrib-watch": "1.1.0",
    "grunt-rtlcss": "2.0.2",
    "grunt-sass": "3.1.0",
    "grunt-stylelint": "0.15.0",
    "grunt-wp-i18n": "1.0.3",
    "jest": "26.6.3",
    "jest-puppeteer": "4.4.0",
    "node-sass": "5.0.0",
    "postcss": "8.1.7",
    "puppeteer": "4.0.1",
    "stylelint": "13.7.2",
    "susy": "2.2.14"
  },
  "engines": {
    "node": ">=0.8.0",
    "npm": ">=1.1.0"
  },
  "dependencies": {}
}
